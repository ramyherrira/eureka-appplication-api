parameters:
  phpVersion: 80100 # PHP 8.1 - Current minimal version supported
  level: max        # Update to higher value in future
  paths:
    - ./src
    - ./tests

  bootstrapFiles:
    - ./vendor/autoload.php

  ignoreErrors:
    - '`Parameter #1 \$content of method (.+)?Controller::getResponseJson\(\) expects array(.+)? given`'
